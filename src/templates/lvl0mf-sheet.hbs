<form class="{{cssClass}}" autocomplete="off">
    <div class="lvl0mf-sheet {{#if actor.flags.lvl0mf-sheet.allow-edit}}allow-edit{{/if}}">
        <nav class="sheet-tabs tabs" data-group="primary>
            <a class='item active' data-tab="sheet-character">{{localize 'LVL0MF.Sheet.Tab.Character.title'}}</a>
            <a class='item' data-tab="sheet-equipment">{{localize 'LVL0MF.Sheet.Tab.Equipment.title'}}</a>
            <a class='item' data-tab="sheet-inventory">{{localize 'LVL0MF.Sheet.Tab.Inventory.title'}}</a>
            <a class='item' data-tab="sheet0notes">{{localize 'LVL0MF.Sheet.Tab.Notes.title'}}</a>
        </nav>

        <section class="tab sheet-character sheet-body" data-group="primary" data-tab="sheet-character">
            <section class="sheet-header">
                <label class="sheet-character-name">
                    <span class="sheet-label">Nom</span>
                    <span class="sheet-value"><input type="text" name="data.character.name" value="{{data.character.name}}"/></span>
                </label>
                <label class="sheet-player-name">
                    <span class="sheet-label">Joueur</span>
                    <span class="sheet-value"><input type="text" name="data.player.name" value="{{data.player.name}}"/></span>
                </label>
                <label class="sheet-race">
                    <span class="sheet-label">Race</span>
                    <span class="sheet-value">
                        <select name="data.attributes.race">
                            <option>Choisir un race</option>
                            <optgroup label="Bases">
                                <option value="Demi-Elfe">Demi-Elfe</option>
                                <option value="Elfe">Elfe</option>
                                <option value="Haufelin" >Haufelin</option>
                                <option value="Humain" >Humain</option>
                                <option value="Nain" >Nain</option>
                            </optgroup>
                            <optgroup label="Anthropomorphes" disabled>
                                <option value="Katacha">Katachas</option>
                                <option value="Canissabilys">Canissabilys</option>
                                <option value="Ekydeös">Ekydeös</option>
                                <option value="Gryvolier">Gryvolier</option>
                                <option value="Zélosaure">Zélosaure</option>
                            </optgroup>
                            <optgroup label="Fantasques" disabled>
                                <option value="Farfadet">Farfadet</option>
                                <option value="Fée">Fée</option>
                                <option value="Gnome">Gnome</option>
                                <option value="Lutin">Lutin</option>
                                <option value="Pooka">Pooka</option>
                            </optgroup>
                            <optgroup label="Goblinoïdes" disabled>
                                <option value="Demi-Ork">Demi-Ork</option>
                                <option value="Gobelin">Gobelin</option>
                                <option value="Hobgobelin">Hobgobelin</option>
                                <option value="Korringan">Korringan</option>
                                <option value="Ork">Ork</option>
                            </optgroup>
                            <optgroup label="Autres monstres" disabled>
                                <option value="Bonhem-Septar">Bonhem-Septar</option>
                                <option value="Cambion">Cambion</option>
                                <option value="Dokälben">Dokälben</option>
                                <option value="Knacker">Knacker</option>
                                <option value="Revenant">Revenant</option>
                            </optgroup>
                        </select>

                        <input type="text" name="data.attributes.race.name" value={{ data.attributes.race.name }} disabled style="display: none;"/>
                    </span>
                </label>
                <label class="sheet-job">
                    <span class="sheet-label">Classe:</span>
                    <span class="sheet-value">
                        <select name="data.attributes.job">
                            <option></option>
                            <option value="Champion" >Champion</option>
                            <option value="Forestier" >Forestier</option>
                            <option value="Guerrier" >Guerrier</option>
                            <option value="Magicien" >Magicien</option>
                            <option value="Voleur" >Voleur</option>
                            <optgroup label="Classes Avancés (remplace magicien)" disabled>
                                <option value="Bouffon" >Bouffon</option>
                                <option value="Élémentaliste" >Élémentaliste</option>
                                <option value="Enchanteur"> Enchanteur</option>
                                <option value="Nécromancien" >Nécromancien</option>
                                <option value="Sorcière" >Sorcière</option>
                            </optgroup>
                        </select>
                        <input type="text" name="data.attributes.job.element" disabled style="display: none;"/>
                    </span>
                </label>

                <label class="sheet-age">
                    <span class="sheet-label">Age</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.age" value="{{data.attributes.age}}" data-dtype="Number" placeholder="13" maxlength="4"/></span>
                </label>
                <label class="sheet-level">
                    <span class="sheet-label">Niveau</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.level" value="{{data.attributes.level}}" data-dtype="Number" placeholder="1" maxlength="4"/></span>
                </label>
                <label class="sheet-size">
                    <span class="sheet-label">Taille</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.size" value="{{data.attributes.size}}" data-dtype="Text" placeholder="1.00" maxlength="10"/></span>
                </label>
            </section>

            <section class="sheet-logo">
                <img alt="Logo LVL0 Médiéval-Fantastique" src="data:image/svg+xml;base64," />
            </section>

            <section class="sheet-stats">
                <section class="sheet-stat">
                    <div class="sheet-label"></div>
                    <div class="sheet-">Base</div>
                    <div class="sheet-">Bonus</div>
                    <div class="sheet-">Armure</div>
                </section>
                <label class="sheet-stat sheet-phy">
                    <span class="sheet-label">Physique</span>
                    <span class="sheet-base"><input type="text" name="data.attributes.phy.base" value="{{data.attributes.phy.base}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                    <span class="sheet-bonus"><input type="text" name="data.attributes.phy.mod" value="{{data.attributes.phy.mod}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                </label>
                <label class="sheet-stat sheet-dex">
                    <span class="sheet-label">Dextérité</span>
                    <span class="sheet-base"><input type="text" name="data.attributes.dex.base" value="{{data.attributes.dex.base}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                    <span class="sheet-bonus"><input type="text" name="data.attributes.dex.mod" value="{{data.attributes.dex.mod}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                    <span class="sheet-armor-mod"><input type="text" name="data.attributes.dex.armor_mod" value="{{data.attributes.dex.armor_mod}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                </label>
                <label class="sheet-stat sheet-int">
                    <span class="sheet-label">Intelligence</span>
                    <span class="sheet-base"><input type="text" name="data.attributes.int.base" value="{{data.attributes.int.base}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                    <span class="sheet-bonus"><input type="text" name="data.attributes.int.mod" value="{{data.attributes.int.mod}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                </label>
                <label class="sheet-stat sheet-cha">
                    <span class="sheet-label">Charisme</span>
                    <span class="sheet-base"><input type="text" name="data.attributes.cha.base" value="{{data.attributes.cha.base}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                    <span class="sheet-bonus"><input type="text" name="data.attributes.cha.mod" value="{{data.attributes.cha.mod}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                </label>
                <label class="sheet-stat sheet-per">
                    <span class="sheet-label">Perception</span>
                    <span class="sheet-base"><input type="text" name="data.attributes.per.base" value="{{data.attributes.per.base}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                    <span class="sheet-bonus"><input type="text" name="data.attributes.per.mod" value="{{data.attributes.per.mod}}" data-dtype="Number" placeholder="1" maxlength="1"/></span>
                </label>
                <section style="display: none;">
                    <input type="number" name="data.attributes.phy" disabled value="{{math data.attributes.phy.base "+" data.attributes.phy.mod}}">
                    <input type="number" name="data.attributes.dex" disabled value="{{math data.attributes.dex.base "+" data.attributes.dex.mod "+" data.attributes.dex.armor_mod}}">
                    <input type="number" name="data.attributes.int" disabled value="{{math data.attributes.int.base "+" data.attributes.int.mod}}">
                    <input type="number" name="data.attributes.cha" disabled value="{{math data.attributes.cha.base "+" data.attributes.cha.mod}}">
                    <input type="number" name="data.attributes.per" disabled value="{{math data.attributes.per.base "+" data.attributes.per.mod}}">
                </section>

                <section class="sheet-mv">
                    <span class="sheet-label">Mouvement</span>
                    <label class="sheet-label">
                        <span class="sheet-phy">Phy</span> + <span class="sheet-dex">Dex</span>
                    </label>
                    <span class="sheet-value"><input type="number" name="data.attributes.mouvement"
                                                    value="{{math data.attributes.phy "+" data.attributes.dex}}"
                                                    disabled/></span>
                </section>
            </section>
            <section class="sheet-health">
                <h3>Point de vie</h3>
                <label class="sheet-health-max">
                    <span class="sheet-label">Maximum</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.hp.max" value="{{data.attributes.hp.value}}" data-dtype="Number" placeholder="0" maxlength="4"/></span>
                </label>
                <label class="sheet-health-damage">
                    <span class="sheet-label">Restant</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.hp.value" value="{{data.attributes.hp.value}}" data-dtype="Number" placeholder="0" maxlength="4"/></span>
                </label>
                <label class="sheet-health-armor">
                    <span class="sheet-label">Armure</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.armor.total" value="{{data.attributes.armor.total}}" data-dtype="Number" placeholder="0" maxlength="2"/></span>
                </label>
                <div class="hp-bar" style="background: linear-gradient(
                            								-90deg,
                            								transparent 0%,
                            								transparent calc(100% - ((100% / ({{#if (eq data.attributes.hp.max 0)}} 1 {{else}} {{data.attributes.hp.max}} {{/if}} + {{#if data.attributes.hp.tempmax}}{{data.attributes.hp.tempmax}}{{else}}0{{/if}} )) * ({{data.attributes.hp.value}} + {{#if data.attributes.hp.temp}}{{data.attributes.hp.temp}}{{else}}0{{/if}})) ),
                            								 	rgba(0,200,0,0.5) calc(100% - ((100% / ({{#if (eq data.attributes.hp.max 0)}} 1 {{else}} {{data.attributes.hp.max}} {{/if}} + {{#if data.attributes.hp.tempmax}}{{data.attributes.hp.tempmax}}{{else}}0{{/if}} )) * ({{data.attributes.hp.value}} + {{#if data.attributes.hp.temp}}{{data.attributes.hp.temp}}{{else}}0{{/if}})) ),
                            								 	rgba(0,200,0,0.5) 100%);">
                </div>
            </section>

            <section class="sheet-magic">
                <h3>Point de mana</h3>
                <label class="sheet-magic-max">
                    <span class="sheet-label">Maximum</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.magic.max" value="{{data.attributes.magic.max}}" data-dtype="Number" placeholder="0" maxlength="3"/></span>
                </label>
                <label class="sheet-magic-left">
                    <span class="sheet-label">Restant</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.magic.value" value="{{data.attributes.magic.value}}" data-dtype="Number" placeholder="0" maxlength="3"/></span>
                </label>
                <label class="sheet-magic-level">
                    <span class="sheet-label">Arcane</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.magic.arcane" value="{{data.attributes.magic.arcane}}" data-dtype="Number" placeholder="0" maxlength="2"/></span>
                </label>
            </section>
            <section class="sheet-xp">
                <h3>Expérience</h3>
                <label class="sheet-xp-value">
                    <span class="sheet-value"><input type="text" name="data.attributes.experience" value="{{data.attributes.experience}}" data-dtype="Number" placeholder="0" maxlength="3"/></span>
                </label>
                <label class="sheet-xp-to-next-level">
                    <span>Prochain niveau:</span>
                    <span class="sheet-value"><input type="text" name="data.attributes.next_level" value="{{data.attributes.next_level}}" data-dtype="Number" placeholder="0" maxlength="2"/></span>
                </label>
            </section>
        </section>

        {{skills.champion.shield_attack.name}}
        <section class="tab sheet-equipment" data-group="primary" data-tab="sheet-equipment">
        </section>
        <section class="tab sheet-equipment" data-group="primary" data-tab="sheet-equipment">
        </section>

        <section class="tab sheet-notes" data-group="primary"  data-tab="sheet-notes">
            <textarea name="data.attributes.notes"></textarea>
        </section>

    </div>
</form>
